{
  "family": "minecraft",
  "taskRoleArn": null,
  "executionRoleArn": "arn:aws:iam::662315753219:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "minecraft",
      "portMappings": [
        {
          "containerPort": 25565,
          "hostPort": 25565,
          "protocol": "tcp/udp"
        },
        {
          "containerPort": 25575,
          "hostPort": 25575,
          "protocol": "tcp/udp"
        }
      ],
      "cpu": 0,
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/minecraft",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "MEMORY",
          "value": "4G"
        },
        {
          "name": "EULA",
          "value": "true"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "minecraftData",
          "containerPath": "/data"
        }
      ]
    }
  ],
  "placementConstraints": [],
  "compatibilities": ["EC2", "FARGATE"],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096",
  "tags": [],
  "volumes": [
    {
      "name": "minecraftData",
      "efsVolumeConfiguration": {
          "fileSystemId": "fs-030cbdce921ca5831",
          "rootDirectory": "/data/minecraft"
      }
    }
  ],
  "pidMode": null,
  "ipcMode": null
}

{
  "family": "minecraft",
  "taskRoleArn": "arn:aws:iam::662315753219:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::662315753219:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "minecraft",
      "portMappings": [
        {
          "containerPort": 25565,
          "hostPort": 25565,
          "protocol": "tcp/udp"
        },
        {
          "containerPort": 25575,
          "hostPort": 25575,
          "protocol": "tcp/udp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/minecraft",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "MEMORY",
          "value": "2G"
        },
        {
          "name": "EULA",
          "value": "true"
        }
      ]
    }
  ],
  "placementConstraints": [],
  "requiresCompatibilities": ["EC2"],
  "cpu": "2048",
  "memory": "2048",
  "tags": [],
  "pidMode": null,
  "ipcMode": null
}
